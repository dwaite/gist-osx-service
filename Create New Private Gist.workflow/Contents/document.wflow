					<key>COMMAND_STRING</key>
					<string>require 'uri'
require 'net/http'

GIST_URL = 'http://gist.github.com/%s.txt'
@proxy = ENV['http_proxy'] ? URI(ENV['http_proxy']) : nil
def write(content, private_gist=false)
	url = URI.parse('http://gist.github.com/gists')
	if @proxy
		req = Net::HTTP::Proxy(@proxy.host, @proxy.port).post_form(url, data(nil, nil, content, private_gist))
	else
		req = Net::HTTP.post_form(url, data(nil, nil, content, private_gist))
	end
	req['Location']
end

def data(name, ext, content, private_gist)
	return {
		'file_ext[gistfile1]'      =&gt; ext,
		'file_name[gistfile1]'     =&gt; name,
		'file_contents[gistfile1]' =&gt; content
	}.merge(private_gist ? { 'action_button' =&gt; 'private' } : {}).merge(auth)
end
 
def auth
	user  = `git config --global github.user`.strip
	token = `git config --global github.token`.strip
	user.to_s.empty? ? {} : { :login =&gt; user, :token =&gt; token }
end

location = write $stdin.read, true
